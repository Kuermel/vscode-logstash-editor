{
	"name": "vscode-logstash-editor",
	"displayName": "Logstash Editor",
	"description": "Provide completion and documentation for Logstash pipeline configuration files",
	"version": "0.1.0",
	"author": {
		"name": "Fabien Baligand",
		"email": "fbaligand@gmail.com"
	},
	"publisher": "fbaligand",
	"license": "Apache-2.0",
	"homepage": "https://github.com/fbaligand/vscode-logstash-editor",
	"bugs": {
		"url": "https://github.com/fbaligand/vscode-logstash-editor/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/fbaligand/vscode-logstash-editor.git"
	},
	"engines": {
		"vscode": "^1.15.0"
	},
	"main": "./out/extension.js",
	"icon": "images/icon-logstash.png",
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"logstash",
		"completion"
	],
	"contributes": {
		"languages": [
			{
				"id": "logstash",
				"aliases": [
					"Logstash",
					"logstash"
				],
				"filenamePatterns": [
					"*logstash.conf",
					"*logstash.conf.j2",
					"*logstash.conf.template",
					"logstash-*.conf"
				]
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.12.18",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"@types/vscode": "^1.15.0"
	}
}
